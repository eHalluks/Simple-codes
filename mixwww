myfunzadaniedwaszescjedendwa = () => {

    let podajslowo = prompt();
    let myarray = [];

    for(let i=0;i<podajslowo.length;i++){
        myarray.push(podajslowo[i])
    }
    
    document.querySelector('.wprowadzoneslowo').innerHTML = myarray;

    for(let x=0;x<myarray.length;x++) {
        for(let y=x+1;y<myarray.length;y++){
            if(myarray[x] > myarray[y]){
                let pomoc;
                pomoc = myarray[x];
                myarray[x] = myarray[y];
                myarray[y] = pomoc;
            }
        }

    }

document.querySelector('.upoWprowadzoneslowo').innerHTML = myarray;

