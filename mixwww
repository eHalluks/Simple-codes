myfuntrescdwaszesctrzy = () => {

    let dwaszesctrzy = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Napisz program który pobierze od usera dwie slownie wpisane liczby a nastepnie wykona obliczenie arytmetyczne a zwroci wynik dodawania."

    document.querySelector('.opisdwaszesctrzy').innerHTML = dwaszesctrzy;

}

myfunzadaniedwaszesctrzy = () => {

    let liczby = new Map() 

    liczby.set("jeden", 1);
    liczby.set("dwa", 2);
    liczby.set("trzy", 3);
    liczby.set("cztery", 4);
    liczby.set("piec", 5);
    liczby.set("szesc", 6);
    liczby.set("siedem", 7);
    liczby.set("osiem", 8);
    liczby.set("dziewiec", 9);
    liczby.set("zero", 0);

    let liczba1 =""; 
    let liczba2 =""; 
    let flaga = true;
    let obliczenia = 0;
    let wynikiobliczen = [];
    let liczbyjeden = [];
    let liczbydwa = [];
    let testpomoc = [];


alert("Hi. It is string arithmetic expression calculator. Please provide only string lowercase. To complete the calculation, enter break.")

do {
    
    liczba1 = prompt("To interrupt, enter the word break. First lowercase number?:")
    if(liczba1 == "break"){
        flaga = false
        break;
    }
    liczbyjeden.push(liczba1)
    

    liczba2 = prompt("Secondary lowercase number?:")
    liczbydwa.push(liczba2)
    obliczenia = liczby.get(liczba1) + liczby.get(liczba2)
    wynikiobliczen.push(obliczenia)

    alert(`Wynik = ${obliczenia}`)


}while(flaga == true); 


for(let x=0;x<wynikiobliczen.length;x++){
    testpomoc.push(" "+liczbyjeden[x]+" + "+liczbydwa[x]+" = "+wynikiobliczen[x]+"  ")
}

document.querySelector('.prezentacjawyrazen').innerHTML = testpomoc;


}

