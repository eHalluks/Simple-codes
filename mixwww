myfunerszesctresc = () => {

    let treszerszesc = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Ktoś podaje pięć liczb jakichkolwiek, a program ma obliczyć sumę ale !!!UWAGA!!!: tylko liczb P.A.R.Z.Y.S.T.Y.C.H !!!!!"

    document.querySelector('.contenterszesc').innerHTML = treszerszesc;

}


myfunerszesc = () => {

    alert("Za chwilę podasz pięć losowych liczb całkowitych. Spróbuj odgadnąć dla jakich liczb program wykonał sumę. Aby przerwać wprowadź 000")

    let suma = 0;
    let liczby =[];
    let flaga = false;

    for(let i=0; i<=5;i++) {
        let podajliczbe = parseInt(prompt("Twoja liczba"));
        if(podajliczbe == 000) {
            flaga = true;
            break;
        }
        liczby.push(podajliczbe)
        if(podajliczbe % 2 === 0) {
            suma += podajliczbe
        }
    }

    if(flaga == false) {
        document.querySelector("#pobraneliczby").innerHTML = "Twoje liczby to: "+"["+liczby+"]";
        document.querySelector("#wynik").innerHTML = "Suma podanych przez ciebie liczb wynosi: "+suma+";";
    } else {
        document.querySelector("#pobraneliczby").innerHTML = "Nic nie podałeś";
        document.querySelector("#wynik").innerHTML = "To nic nie obliczyłem :-)";
    }


}

myfunerszescdodatek = () => {

        let odpowiedz = parseInt(prompt("Podaj odpowiedź: << 1-Podzielne przez 2 >>, << 2-niepodzielne przez 2 >>; << 3-inne >>"))
        ;

        if(odpowiedz === 1) {
        let odptrue = "Brawo, to jest poprawna odpowiedź"
        document.querySelector("#odp").innerHTML = odptrue;
        } else {
            let odpfalse = ("Niestety, to nie jest poprawna odpowiedź. Właściwa odpowiedź to podzielne przez 2")
            document.querySelector("#odp").innerHTML = odpfalse;
        }
}



