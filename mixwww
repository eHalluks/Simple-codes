myfuntrescczteryszesctrzy = () => {

    let czteryszesctrzy = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Napisz program który w zależności od podanego przez użytkownika wzrostu w cm wypisze odpowiedni tekst."

