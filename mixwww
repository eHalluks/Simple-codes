myfuntrzysiedemcztery = () => {

    let trzysiedemcztery = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Napisz program który policzy pole figury którą wybierze user."

    document.querySelector('.contenttrzysiedemcztery').innerHTML = trzysiedemcztery;

