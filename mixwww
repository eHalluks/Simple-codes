myfuntrescSzescSiedemJeden = () => {

    let szescSiedemJeden = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Zrob tak zeby dane ktore wprowadza sie w formularzu pojawily sie w polach po prawej."

    document.querySelector('.opisSzescSiedemJeden').innerHTML = szescSiedemJeden;

}

myfunZadanieSzescSiedemJeden = () => {

    //Tutaj pobieram imię
    let imie = document.querySelector("input[name='imie']").value;

    //Tutaj wstawiam imie w span
    document.querySelector("span#imie").innerHTML = imie;

    //Tutaj pobieram nazwisko
    let nazwisko = document.querySelector("input[name='nazwisko']").value;

    //Tutaj wstawiam nazwisko w span
    document.querySelector("span#nazwisko").innerHTML = nazwisko;

    //Tutaj pobieram z listy rozwijanej
    let miasto = document.querySelector("select[name='miasto']").value;

    //tutaj robie pentle na checkbox i pobieram value
    let znajmoscTech = document.querySelectorAll("input[type='checkbox']");
    let wyborcheckbox = [];
    for(let y of znajmoscTech){
        if(y.checked){
            wyborcheckbox.push(" "+y.value+" ")

            
        }
    }

    //Tutaj wstawiam checkboxy
    document.querySelector("span#technologie").innerHTML = wyborcheckbox;

    //Tutaj wstawiam z listy rozwijanej
    document.querySelector("span#miasto").innerHTML = miasto;

    //Tutaj robię pętle na radio i pobieram value
    let stanowisko = document.querySelectorAll("input[name='stanowisko']");
    let wyborstanowiska;
    for(let x of stanowisko){
        if(x.checked == true){
            wyborstanowiska = x.value
            break;
        }
    }

    //Tutaj wstawiam wartosci z petli stanowisko
    document.querySelector("span#stanowisko").innerHTML = wyborstanowiska;


    //Tutaj pobieram wartosc z textarea
    let uwagi = document.querySelector("textarea[name='uwagi']").value;

    //Tutaj wstawiam wartosc z textarea
    document.querySelector("span#uwagi").innerHTML = uwagi;

}


