myfuntreszerjedenascie = () => {

    let trescerjedenascie = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Znowu program pobiera od usera 5 liczb na koncu user podaje 0 ale to nie wlicza sie do calosci. Wyswietl sume, max, min ilosc parz, ilosc nie parz, srednia arytm, i na koniec wypisz wszystkie wartosci."

    document.querySelector('.contententjedenascie').innerHTML = trescerjedenascie;

