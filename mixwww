myfuntrzysiedemczteryzadanie = () => {

    let coChceLiczyc = prompt("Liczymy pole figury. Podaj jedną z dwóch figór: trójkąt lub prostokąt")
    let odpowiedz = coChceLiczyc;
    
    if(odpowiedz == "trójkąt") {

        let dlPodstawy = parseInt(prompt("Długość podstawy:"));
        document.querySelector('.dlugoscpodstawy').innerHTML = "Długość podstawy trójkąta: "+dlPodstawy+"cm";

        let wysTrój = parseInt(prompt("Wysokość trójkąta;"));
        document.querySelector('.wysokosctrojkata').innerHTML = "Wysokość trójkąta: "+wysTrój+"cm";

        let obliczeniaDlWys = dlPodstawy * wysTrój / 2;
        document.querySelector('.obliczeniapolatrojkata').innerHTML = "Pole trójkąta wynosi: "+obliczeniaDlWys+"cm";

        
    }else if(odpowiedz == "prostokąt") {
        let dlBokuA = parseInt(prompt("Długość boku a:"))
        document.querySelector('.dlugoscBokuA').innerHTML = "Długość boku a: "+dlBokuA+"cm";

        let dlBokuB = parseInt(prompt("Długość boku b:"))
        document.querySelector('.dlugoscBokuB').innerHTML = "Długość boku b: "+dlBokuB+"cm";

        let obliczeniaAb = dlBokuA * dlBokuB;
        document.querySelector('.obliczeniaPolaProstokata').innerHTML = "Pole prostokąta wynosi: "+obliczeniaAb+"cm";
    }
}



