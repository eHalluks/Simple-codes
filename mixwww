myfuntrescszescszescpiec = () => {

    let szescszescpiec = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Napisz program który stworzy dwie tablice a nastepnie wskaze najwieksza wspolna wartosc."

    document.querySelector('.opisszescszescpiec').innerHTML = szescszescpiec;

}

myfunzadanieszescszescpiec = () => {


    let tab1 = []; //los1
    let tab2 = []; //los2
    
    let tab3 = []; //takie same
    
    
    for(let x = 0; x < 10; x++){
            let los1 = Math.floor((Math.random() * 20)+ 1);
            tab1.push(los1);
            let los2 = Math.floor((Math.random() * 20)+ 1);
            tab2.push(los2);
        }
        
        console.log("Ciągi nie uporządkowane")
        console.log(`Pierwsza tablica ${tab1}`);
        console.log(`Druga tablica ${tab2}`);
        console.log("                      ");
        
    
        function sortuj(dane){
    
            for(let i = 0; i < dane.length -1; i++){
                for(let j=i+1; j < dane.length; j++){
                    if(dane[i] > dane[j]) {
                        let pomoc;
                        pomoc = dane[i]
                        dane[i] = dane[j];
                        dane[j] = pomoc; 
                    }  
                    
                }
            }
        }
    
        //sort
    
        for(let d=0;d<10;d++){
            for(let s=0;s<10;s++)
            if(tab1[s] === tab2[d]){
                tab3.push(tab1[s])
            }
        }
        
        sortuj(tab1);
        sortuj(tab2);
    
        console.log("Ciągi uporządkowane")
        console.log(`Pierwsza tablica ${tab1}`);
        console.log(`Druga tablica ${tab2}`);
        console.log("                      ");
    
        sortuj(tab3);
        console.log(tab3);
        console.log("                      ");
    
        const lastItemone = tab1[tab1.length - 1];
        const lastItemtwo = tab2[tab2.length - 1];
        const lastItemthree = tab3[tab3.length - 1];
    
    document.querySelector('.oznaczeniedlaciagu').innerHTML ="Ciąg uporządkowany" ;
    document.querySelector('.wylosowaneTab1').innerHTML ="Pierwsza tablica: "+"["+tab1+"]";
    document.querySelector('.wylosowaneTab2').innerHTML ="Druga tablica: "+"["+tab2+"]";

    if(tab3 == "") {
        if(lastItemone > lastItemtwo){
        document.querySelector('.najwiekszawspolna').innerHTML = `Największa wartość znajduje się w Tablicy pierwszej ${lastItemone}`;

        } else if(lastItemone < lastItemtwo) {
            document.querySelector('.najwiekszawspolna').innerHTML = `Największa wartość znajduje się w Tablicy pierwszej ${lastItemtwo}`;
        } else if(lastItemone == lastItemtwo) {
            document.querySelector('.najwiekszawspolna').innerHTML = `W obydwu tablicach wystąpiła taka sama najwyższa wartość i wynosi ${lastItemtwo}.`;
        }
    } else
    document.querySelector('.najwiekszawspolna').innerHTML = `Największa wspólna wartość dla obu tablic to: ${lastItemthree}`;

}

