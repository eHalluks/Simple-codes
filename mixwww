myfuntrzysiedemtrzy = () => {

    let trzysiedemtrzy = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Napisz program który policzy pole lub obwód prostokąta."

    document.querySelector('.contenttrzysiedemtrzy').innerHTML = trzysiedemtrzy;

