myfuntrescdwaszescjeden = () => {

    let dwaszescjeden = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Napisz program który wypełni tablicę 10 elementową losowymi wartościami od 1-100."

    document.querySelector('.opisdwaszescjeden').innerHTML = dwaszescjeden;


}

myfunzadaniedwaszescjeden = () => {


    let mynumbers = [];

    for(let i=0;i<=10;i++) {
        let losowanie = Math.floor((Math.random() * 100)+ 1);
        mynumbers.push(losowanie)
    }

    for(let x = 0; x < mynumbers.length -1; x++){
        for(let y=x+1; y < mynumbers.length; y++){
            if(mynumbers[x] < mynumbers[y]) {
                let pomoc;
                pomoc = mynumbers[x]
                mynumbers[x] = mynumbers[y];
                mynumbers[y] = pomoc; 
            }
        }
    }
    document.querySelector('.typeofsortHigh').innerHTML = "Poniżej sort od największego do najmniejszego:";
    document.querySelector('.viewnumbers').innerHTML = "["+mynumbers+"]";
    
    
    for(let x = 0; x < mynumbers.length -1; x++){
        for(let y=x+1; y < mynumbers.length; y++){
            if(mynumbers[x] > mynumbers[y]) {
                let pomoc;
                pomoc = mynumbers[x]
                mynumbers[x] = mynumbers[y];
                mynumbers[y] = pomoc; 
            }
        }
    }
    document.querySelector('.typeofsortlow').innerHTML = "Ponieżej sort od najmniejszego do największego:";
    document.querySelector('.viewnumberst2').innerHTML = "["+mynumbers+"]";

}

