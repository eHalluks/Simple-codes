myfuntrzysiedemtrzyzadanie = () => {


alert("Coś będziemy liczyć. Dowiesz się już za chwilę. Podążaj za komunikatami. Zaczynamy!");

let wartosc1 = parseInt(prompt("Ile wynosi pierwszy bok prostokąta?"));
document.querySelector('.firstsiderectangle').innerHTML = "Podana wartość boku x: "+wartosc1+"cm";

let wartosc2 = parseInt(prompt("Ile wynosi drugi bok prostokąta?"));
document.querySelector('.secondsiderectangle').innerHTML = "Podana wartość boku y: "+wartosc2+"cm";

let userchoises = parseInt(prompt("Wybierz co chcesz policzyć: 1-Pole, 2-obwód"));

if(userchoises == 1){
    document.querySelector('.choisetheuser').innerHTML 
    ="Wybrałeś: "+userchoises+". Będziemy liczyć pole.";
} else {
    document.querySelector('.choisetheuser').innerHTML 
    ="Wybrałeś: "+userchoises+". Będziemy liczyć obwód.";
}

let obliczeniaPola = null;
let obliczeniaObwodu = null;

if(userchoises == 1) {
    obliczeniaPola = wartosc1 * wartosc2;
    document.querySelector('.arearectangle').innerHTML 
    ="Pole prostokąta wynosi: "+obliczeniaPola+"cm";
}

if (userchoises == 2) {
    obliczeniaObwodu = 2 *(wartosc1 + wartosc2);
    document.querySelector('.circumferencerectangle').innerHTML ="Obwód prostokąta wynosi: "+obliczeniaObwodu+"cm";
}

}

