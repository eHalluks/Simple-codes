myfunfivetwoone = () => {

    let tresczadaniapiecdwajeden = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Utwórz zmienne, zgodnie z treścią:(zm1=SUKCES; zm2=występuje; zm3=WYSIŁKIEM; zm4 =gdzie; zm5 =przed; zm6 =słownik; zm7 =jest; zm8 =Jedynym; zm9 =miejscem)a nastepnie ulóż zdanie: Jedynym miejscem gdzie SUKCES wystepuje przed WYSYŁKIEM jest słownik"

    document.querySelector('.contentpiecdwajeden').innerHTML = tresczadaniapiecdwajeden;
}


myfundwa_fivetwoone = () => {


    let zm1 = "SUKCES";
    let zm2 = "występuje";
    let zm3 = "WYSIŁKIEM";
    let zm4 = "gdzie";
    let zm5 = "przed";
    let zm6 = "słownik";
    let zm7 = "jest";
    let zm8 = "Jedynym";
    let zm9 = "miejscem";
    let pomoc = "";
    
    let wyswietlanie = [zm1,zm2,zm3,zm4,zm5,zm6,zm7,zm8,zm9]

    document.querySelector('#przedzmiana').innerHTML = wyswietlanie;


    //Jedynym miejscem gdzie SUKCES występuje przed WYSIŁKIEM jest słownik
    //zm8, zm9, zm4, zm1, zm2, zm5, zm3, zm7, zm6

    //## = container
    //P-pomoc
    //E-empty_container

    pomoc = zm1; //P-sukces
    
    zm1 = zm8; //e-zm8 ## ________ jedynym
    
    zm8 = pomoc; //SUKCES
    
    pomoc = zm2; //p-występuje
    
    zm2 = zm9;//e-zm9 ## ________ miejscem
    
    zm9 = pomoc;//WYSTĘPUJE

    pomoc = zm3; //P-wysiłkiem

    zm3 = zm4;//e-zm4 ## ________ gdzie

    zm4 = zm8;//e-zm8 ## ________ SUKCES

    zm8 = pomoc //wysiłkiem

    pomoc = zm5; //p-przed

    zm5 = zm9;//e-zm9 ## ________ wystepuje

    zm9 = pomoc; //przed

    pomoc = zm6;//p-slownik

    zm6 = zm9; //e-zm9 ## ________ przed

    zm9 = pomoc; //slownik

    pomoc = zm7;

    zm7 = zm8;  //e-zm8 ## ________ wysilkiem

    zm8 = pomoc; // ## ________ wysilkiem
    
    let wyswietlania2 = [zm1,zm2,zm3,zm4,zm5,zm6,zm7,zm8,zm9];

    document.querySelector("#pozmianie").innerHTML = wyswietlania2;
}

