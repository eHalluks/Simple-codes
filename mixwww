myfuntreszerdziesiec = () => {

    let treszerdziesiec = "P.A.M.I.Ę.T.A.J - CZYTAJ ZE ZROZUMIENIEM: Program ma losować liczby od (1 do 10) i od razu ma losować znaki takie jak: (+, -, *). Konstrukcja działania ma być prezentowana userowi z pytaniem jaka jest odpowiedź. Po udzielniu wszystkich odpowiedzi program ma prezentować dane podzielone na odpowiedzi poprawne i nie poprawne."

    document.querySelector('.contenterdziesiec').innerHTML = treszerdziesiec;

}




myfunpodajwynikdzialania = () => {


    let tblpoprawne = [];
    let tblbledne = [];
    let pytania = [];
    let tblwlasciwaodp = [];
    let tbltwojaodpowiedz = [];

    let zestawpoprawnych = new Map();
    let zestawblednych = new Map();
    let licznikpop = 0;
    let licznikniepop = 0;
    
    
    let znak1 = 1; //dodawanie
    let znak2 = 2; //odejmowanie
    let znak3 = 3; //mnozenie
    
    
    for(let i=0; i<3; i++) {
    
        let los1 = Math.floor((Math.random() * 5) + 1);
        let los2 = Math.floor((Math.random() * 5) + 1);
        let los3 = Math.floor((Math.random() * 3) + 1);
    
        let oblicz, mojaodp;
    
    
        if(los3 == 1) {
            oblicz = los1 + los2;
            tblwlasciwaodp.push(oblicz);
            mojaodp = parseInt(prompt(`Ile to jest? ${los1} + ${los2}`));
            pytania.push(`Ile to jest: ${los1} + ${los2} = ?`);
            tbltwojaodpowiedz.push(mojaodp);
        }
        else if (los3 == 2) {
            oblicz = los1 - los2;
            tblwlasciwaodp.push(oblicz);
            mojaodp = parseInt(prompt(`Ile to jest? ${los1} - ${los2}`));
            pytania.push(`Ile to jest: ${los1} - ${los2} = ?`);
            tbltwojaodpowiedz.push(mojaodp);
        }
        else if (los3 == 3) {
            oblicz = los1 * los2;
            tblwlasciwaodp.push(oblicz);
            mojaodp = parseInt(prompt(`Ile to jest? ${los1} * ${los2}`));
            pytania.push(`Ile to jest: ${los1} * ${los2} = ?`);
            tbltwojaodpowiedz.push(mojaodp);
        }
        
        if(mojaodp == oblicz) {
            tblpoprawne.push(mojaodp);
            tblbledne.push("");
            
        } else {
            tblpoprawne.push("");
            tblbledne.push(mojaodp);
        }
    
    }

    for(let i=0; i<pytania.length; i++){
        console.log(pytania[i]);
        if(tblpoprawne[i] != ""){
            console.log("Twoje poprawne odpowiedzi to " +tblpoprawne[i]);
            zestawpoprawnych.set(pytania[i],tblpoprawne[i]);
            licznikpop++
        }
        else{
            console.log("Twoje błędne odpowiedzi to " +tblbledne[i]);
            zestawblednych.set(pytania[i], tblbledne[i]);
            licznikniepop++
    }

    
    console.log(zestawpoprawnych.get(pytania[i]))
    console.log(zestawblednych.get(pytania[i]))
    
}

console.log(tblwlasciwaodp)

document.querySelector('#pyt1').innerHTML ="Pytanie pierwsze: "+pytania[0];
document.querySelector('#odpwlasciwa1').innerHTML ="Właściwa odpowiedź to: "+tblwlasciwaodp[0];
document.querySelector('#twaodp1').innerHTML ="Twoja odpowiedź: "+tbltwojaodpowiedz[0];

document.querySelector('#pyt2').innerHTML ="Pytanie drugie: "+pytania[1];
document.querySelector('#odpwlasciwa2').innerHTML ="Właściwa odpowiedź to: "+tblwlasciwaodp[1];
document.querySelector('#twaodp2').innerHTML ="Twoja odpowiedź: "+tbltwojaodpowiedz[1];

document.querySelector('#pyt3').innerHTML ="Pytanie i trzecie: "+pytania[2];
document.querySelector('#odpwlasciwa3').innerHTML ="Właściwa odpowiedź to: "+tblwlasciwaodp[2];
document.querySelector('#twaodp3').innerHTML ="Twoja odpowiedź: "+tbltwojaodpowiedz[2];

}



