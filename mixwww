myfunuruchompodawajke = () => {

    alert("Podaj tyle ile chcesz liczb całkowitych od ujemnych do dodatnich. Aby zakończyć podawanie wprowadź liczbę 0")

    let licznik = 0;
    let tab1 = [];
    let wajha = true;
    let min;
    let max0;
    let suma = 0;
    let srednia;
    let licznikparzystych = 0;
    let liczniknieprzystych = 0;
    let licznikwiekszeodzera = 0;
    let licznikmniejszeodzera = 0;
    let tab1posortowane = [];
    

    do{
        let prPliczby = parseInt(prompt("Aby zakończyć wprowadź 0"));
        if(prPliczby == 0){
            break;
        }
        if(licznik == 0) {
            min = prPliczby;
            max = prPliczby;
        }else if (prPliczby > max) {
            max = prPliczby
        } else if (prPliczby < min) {
            min = prPliczby
        }

        if(prPliczby % 2 == 0) {
            licznikparzystych++
        } else if(prPliczby % 2 != 0) {
            liczniknieprzystych++
        }if(prPliczby < 0) {
            licznikmniejszeodzera++
        }else if(prPliczby > 0) {
            licznikwiekszeodzera++
        }
        licznik++
        suma += prPliczby
        tab1.push(prPliczby);

    } while(true);

    srednia = suma / licznik


    document.querySelector('.suma').innerHTML = "Suma liczba: "+suma;
    document.querySelector('.min').innerHTML = "Wartość min: "+min;
    document.querySelector('.max').innerHTML = "Wartość max: "+max;
    document.querySelector('.parzyste').innerHTML = "Ilość parzystych: "+licznikparzystych;
    document.querySelector('.nieparzyste').innerHTML ="Ilość nie parzystych: "+liczniknieprzystych;
    document.querySelector('.wiekszeOdZera').innerHTML ="Ilość większych od zera: "+licznikwiekszeodzera;
    document.querySelector('.mniejszeOdZera').innerHTML ="Ilość mniejszych od zera: "+licznikmniejszeodzera;
    document.querySelector('.srednia').innerHTML ="Średnia arytmetyczna: "+srednia;
    document.querySelector('.iloscprowadzonych').innerHTML ="Ilość wprowadzonych ogółem: "+licznik;
    document.querySelector('.wszystkowyswietl').innerHTML ="Nie uporządkowane Wartości jakie wprowadzono: "+tab1;

    for(let i = 0; i < tab1.length -1; i++){
        for(let j=i+1; j < tab1.length; j++){
            if(tab1[i] > tab1[j]) {
                let pomoc;
                pomoc = tab1[i]
                tab1[i] = tab1[j];
                tab1[j] = pomoc; 
            }
        }
    }

    document.querySelector('.wszystkowyswiet2').innerHTML ="Uporządkowane od najmniejszego wartości jakie wprowadzono: "+tab1;

    for(let i = 0; i < tab1.length -1; i++){
        for(let j=i+1; j < tab1.length; j++){
            if(tab1[i] < tab1[j]) {
                let pomoc;
                pomoc = tab1[i]
                tab1[i] = tab1[j];
                tab1[j] = pomoc; 
            }
        }
    }

    document.querySelector('.wszystkowyswiet3').innerHTML ="Uporządkowane od największego wartości jakie wprowadzono: "+tab1;
    
    
    console.log("Suma:",suma);
    console.log("Min:",min);
    console.log("Max:",max);
    console.log("Parzyste ",licznikparzystych);
    console.log("Nieparzyste: ",liczniknieprzystych);
    console.log("Ilość wiekszych od zera: ",licznikwiekszeodzera);
    console.log("Ilość mniejszych od zera: ",licznikmniejszeodzera);
    console.log("Średnia: ",srednia);
    console.log("Ilość wprowadzonych liczb:",licznik);
    console.log(tab1);
}
