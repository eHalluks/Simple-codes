myfunzadanieczteryszesctrzy = () => {


let podajwzrost = parseInt(prompt("Podaj wzost"));
let level1 = "0-164cm - Niski wzrost"
let level2 = "165-174cm - Średni wzrost"
let level3 = "175cm > - Wysok wzrost"

document.querySelector('.tocowpisal').innerHTML = "Podałeś: "+podajwzrost+"cm";

if (podajwzrost > 0 && podajwzrost < 165 ) {
    document.querySelector('.lowgrowth').innerHTML = level1;
    document.querySelector('.ciekawostkaniski').innerHTML ="W historii za najniższego człowieka na świecie uchodzi Chandra Bahadur Dangi, który przyszedł na świat 30 listopada 1939 roku w Nepalu, a zmarł 4 września 2015 roku w Pago Pago. Mężczyzna ważył zaledwie 14,5 kilograma, a jeśli chodzi o wzrost, mierzył 0,546 metra.";

} else if (podajwzrost >= 165 && podajwzrost < 175) {
    document.querySelector('.middlegrowth').innerHTML = level2;
    document.querySelector('.ciekawostkasredni').innerHTML = "Średni wzrost kobiet na świecie wynosi 159,5 cm.";


} else if (podajwzrost > 175) {
    document.querySelector('.highgrowth').innerHTML = level3;
    document.querySelector('.ciekawostkawysoki').innerHTML = "Według Księgi Rekordów Guinnessa najwyższym obecnie człowiekiem na świecie jest 38-letni Turek Sultan Kosen, który mierzy 2,51 m. Najwyższym człowiekiem we współczesnej historii jest Amerykanin Robert Wadlow, który miał 2,72 m wzrostu.";
    
}

}

